<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="ee.ut.math.tvt.salessystem.domain.data.HistoryItem" table="PUBLIC.HISTORYITEM">
   <id name="id" type="long" column="ID" >
   <generator class="assigned"/>
  </id>
  <property name="DATETAKEN" type="String">
   <column name="DATE" />
  </property>
  <property name="TIMETAKEN" type="String">
        <column name="TIME" />
  </property>
  <property name="totalOrderPrice" type="double">
  <column name="HISTORY_PRICE"/>
  </property>
  <property name="soldItemList>
  </property>
 </class>
</hibernate-mapping>